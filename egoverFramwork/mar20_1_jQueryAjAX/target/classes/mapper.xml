<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.we.mar201.menu.Mapper">
	<select id="getData" resultType="com.we.mar201.menu.Menu">
		SELECT * from mar20_menu
	</select>
	<insert id="reg" parameterType="com.we.mar201.menu.Menu">
		insert into mar20_menu values(#{m_name},#{m_price})
	</insert>
	
	<select id="search" parameterType="com.we.mar201.menu.Menu" resultType="com.we.mar201.menu.Menu">
		select * from mar20_menu where m_name like '%'||#{m_name}||'%' order by m_name
	</select>
</mapper>
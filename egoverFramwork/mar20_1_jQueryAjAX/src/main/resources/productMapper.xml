<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.we.mar201.product.ProductMapper">

	
	<select id="allProducts" resultType="int">
		select count(*) from
		mar20_product
	</select>
	
 	<select id="getProduct"
		resultType="com.we.mar201.product.Product" parameterType="java.util.HashMap">
		select * from(
		select rownum as rn, m_name, m_price from(
		select m_name,m_price from mar20_product order by m_name
		)
		)where rn &gt;=#{start} and rn &lt;=#{end}
	</select>
</mapper>
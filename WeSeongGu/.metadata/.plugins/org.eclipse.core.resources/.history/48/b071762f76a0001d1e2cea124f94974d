package com.we.jan30.main;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.PreparedStatement;
import java.util.Scanner;

public class SelectMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String addr = "jdbc:oracle:thin:@192.168.0.170:1521:xe";
		Scanner k = new Scanner(System.in);

		Connection con = null;
		PreparedStatement pstmt = null;
		try {
			// 연결-------------------------------------------------------
			con = DriverManager.getConnection(addr, "we", "3750");
			System.out.println("연결 성공");

			String sql = "select m_price, m_price from jan30_product order by m_name, m_price";
			
			pstmt = con.prepareStatement(sql);
			
			String row = pstmt.// 안동입력시 작동을 안함 그럼 그냥 껐다가 그냥 다시 키면 된다고함
			System.out.println(row);

		} catch (Exception e) {
			e.printStackTrace();
		}

		try { pstmt.close(); } catch (Exception e1) {}
		try {con.close();} catch (Exception e) {}
		k.close();
	}

}
